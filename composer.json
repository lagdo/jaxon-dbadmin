{
    "name": "lagdo/jaxon-dbadmin",
    "type": "application",
    "description": "Database management dashboard based on Jaxon and Adminer",
    "authors": [
        {
            "name": "Thierry Feuzeu",
            "email": "thierry.feuzeu@gmail.com"
        }
    ],
    "homepage": "https://github.com/lagdo/jaxon-dbadmin",
    "license": "Apache-2.0",
    "keywords": ["Database", "Admin", "Sql", "Dashboard", "Jaxon", "Ajax",
        "PostgreSQL", "MySQL", "Sqlite", "Oracle", "MsSQL", "MongoDb"],
    "support": {
        "issues": "https://github.com/lagdo/jaxon-dbadmin/issues",
        "source": "https://github.com/lagdo/jaxon-dbadmin"
    },
    "require": {
        "php": ">=7.1.0",
        "ext-json": "*",
        "ext-iconv": "*",
        "lagdo/dbadmin-driver": "dev-main",
        "lagdo/ui-builder": "dev-main",
        "jaxon-php/jaxon-core": "dev-main",
        "jaxon-php/jaxon-dialogs": "dev-main",
        "jaxon-php/jaxon-annotations": "dev-main"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "behat/behat": "^3.10",
        "dvdoug/behat-code-coverage": "5.0.*",
        "php-coveralls/php-coveralls": "^2.5",
        "phpunit/phpcov": "^8.2"
    },
    "autoload": {
        "psr-4": {
            "Lagdo\\DbAdmin\\App\\": "app/",
            "Lagdo\\DbAdmin\\Db\\": "src/db/",
            "Lagdo\\DbAdmin\\Ui\\": "src/ui/",
            "Lagdo\\DbAdmin\\": "src/lib/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Lagdo\\DbAdmin\\Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
