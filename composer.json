{
    "name": "lagdo/jaxon-dbadmin",
    "type": "application",
    "description": "Database management dashboard based on Jaxon and Adminer",
    "authors": [
        {
            "name": "Thierry Feuzeu",
            "email": "thierry.feuzeu@gmail.com"
        }
    ],
    "homepage": "https://github.com/lagdo/jaxon-dbadmin",
    "license": "Apache-2.0",
    "keywords": ["Database", "Admin", "Sql", "Dashboard", "Jaxon", "Ajax", "PostgreSQL", "MySQL", "Sqlite"],
    "support": {
        "issues": "https://github.com/lagdo/jaxon-dbadmin/issues",
        "source": "https://github.com/lagdo/jaxon-dbadmin"
    },
    "require": {
        "php": ">=8.0.0",
        "ext-json": "*",
        "ext-iconv": "*",
        "jaxon-php/jaxon-core": "^5.2",
        "jaxon-php/jaxon-attributes": "^1.0",
        "jaxon-php/jaxon-dialogs": "^5.0",
        "jaxon-php/jaxon-storage": "^1.1",
        "jaxon-php/jaxon-upload": "^2.1",
        "lagdo/dbadmin-driver": "^0.18",
        "lagdo/facades": "^1.0",
        "lagdo/ui-builder": "^0.2",
        "lagdo/jaxon-ui-builder": "^0.1"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "behat/behat": "^3.10",
        "dvdoug/behat-code-coverage": "5.0.*",
        "php-coveralls/php-coveralls": "^2.5",
        "phpunit/phpcov": "^8.2"
    },
    "autoload": {
        "psr-4": {
            "Lagdo\\DbAdmin\\Ajax\\": "app/ajax/",
            "Lagdo\\DbAdmin\\Ui\\": "app/ui/",
            "Lagdo\\DbAdmin\\Db\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Lagdo\\DbAdmin\\Tests\\": "tests/"
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": false
}
